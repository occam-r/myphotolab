    "expo-keep-awake": "~14.1.4",
    "expo-local-authentication": "~16.0.4",
    "expo-media-library": "~17.1.6",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.2",
